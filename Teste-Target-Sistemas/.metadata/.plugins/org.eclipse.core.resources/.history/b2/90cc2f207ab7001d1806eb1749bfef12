import javax.swing.JOptionPane;

public class Teste02 {
    public static void main(String[] args) {
        int num = Integer.parseInt(JOptionPane.showInputDialog("Digite o número desejado: "));

        int numeroAnterior = 0;
        int numeroAtual = 1;
        int proximoNumero = numeroAnterior + numeroAtual;

        while (proximoNumero <= numero) {
            if (proximoNumero == numero) {
                System.out.println(numero + " pertence à sequência de Fibonacci.");
                return;
            }
            numeroAnterior = numeroAtual;
            numeroAtual = proximoNumero;
            proximoNumero = numeroAnterior + numeroAtual;
        }

        System.out.println(numero + " não pertence à sequência de Fibonacci.");
    }
}

